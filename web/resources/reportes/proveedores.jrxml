	<queryString>
		<![CDATA[SELECT
     proveedor."rifproveedor" AS proveedor_rifproveedor,
     proveedor."razonsocial" AS proveedor_razonsocial,
     proveedor."direccionfiscal" AS proveedor_direccionfiscal,
     proveedor."telefonos" AS proveedor_telefonos,
     proveedor."correo" AS proveedor_correo,
     proveedor."fechainscripcion" AS proveedor_fechainscripcion,
     proveedor."personacontacto" AS proveedor_personacontacto,
     proveedor."telefonocontacto" AS proveedor_telefonocontacto,
     proveedor."idtipoproveedor" AS proveedor_idtipoproveedor,
     proveedor."idpersonalidad" AS proveedor_idpersonalidad,
     proveedor."idusuario" AS proveedor_idusuario,
     proveedor."idresidencia" AS proveedor_idresidencia,
     proveedor."idcontribuyente" AS proveedor_idcontribuyente,
     proveedor."iddocumento" AS proveedor_iddocumento,
     tipoproveedor."idtipoproveedor" AS tipoproveedor_idtipoproveedor,
     tipoproveedor."tipoproveedor" AS tipoproveedor_tipoproveedor,
     personalidadjuridica."idpersonalidad" AS personalidadjuridica_idpersonalidad,
     personalidadjuridica."personalidad" AS personalidadjuridica_personalidad,
     personalidadjuridica."abreviatura" AS personalidadjuridica_abreviatura,
     empresa."idempresa" AS empresa_idempresa,
     empresa."rif" AS empresa_rif,
     empresa."razonsocial" AS empresa_razonsocial,
     empresa."nombrecomercial" AS empresa_nombrecomercial,
     empresa."direccionfiscal" AS empresa_direccionfiscal,
     empresa."telefonos" AS empresa_telefonos
FROM
     "public"."tipoproveedor" tipoproveedor INNER JOIN "public"."proveedor" proveedor ON tipoproveedor."idtipoproveedor" = proveedor."idtipoproveedor"
     INNER JOIN "public"."personalidadjuridica" personalidadjuridica ON proveedor."idpersonalidad" = personalidadjuridica."idpersonalidad",
     "public"."empresa" empresa
WHERE
     empresa."idempresa" = $P{idempresa}   ORDER BY proveedor."razonsocial"]]>
	</queryString>
